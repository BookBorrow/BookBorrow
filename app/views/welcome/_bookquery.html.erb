<h2>Please select the book that you are lending</h2>
<table id="bookresultspartial" class="table-responsive table-bordered">
  <% counter = 0 %>
  <tr>
    <% @books[0..9].each do |book|%>
    <td class="bookitem", data-ISBN="<%=book.isbn%>">
      <%= image_tag(book.cover_url, :class => "setISBN")%>
    </td>
    <% counter+=1 %>
    <% if counter %5 ==0 %>
  </tr>
  <tr>
    <% end %>
    <% end %>
  </tr>
</table>
<% if @books.length>10 %>
<h4>Not enough data damnit. </h4>
<% end %>